{
    "mem0": {
        "llm": {
            "provider": "ollama",
            "config": {
                "model": "qwen3:8b",
                "temperature": 0.1,
                "max_tokens": 2000,
                "ollama_base_url": "http://ollama:11434"
            }
        },
        "embedder": {
            "provider": "ollama",
            "config": {
                "model": "bge-m3",
                "embedding_dims": 1024,
                "ollama_base_url": "http://ollama:11434"
            }
        },
        "graph_store": {
            "provider": "neo4j",
            "config": {
                "url": "env:NEO4J_URL",
                "username": "env:NEO4J_USERNAME",
                "password": "env:NEO4J_PASSWORD",
                "database": "neo4j",
                "base_label": true
            },
            "threshold": 0.75
        }
    }
}
